{
  "name": "eek-whales",
  "version": "0.1.0",
  "description": "a collection of JS/TS modules for functional programming",
  "type": "module",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "scripts": {
    "build": "tsup src/index.ts src/index.spec.ts --format esm --dts --sourcemap --no-splitting --clean",
    "dev": "pnpm run build --watch",
    "test": "pnpm run build --silent --onSuccess 'node dist/index.spec.js'",
    "clean": "rm -rf dist node_modules"
  },
  "author": "Rand Schneck <i@rasch.co>",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.14.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  }
}
